[[runners]]
  name = "{{ hostname }}/{{ platform }}:{{ kind }}"
  url = "{{ url }}"
  token = "{{ token }}"
  executor = "docker"
  image = "{{ docker_repository_url  }}edgetoolkit/gcc5"
  limit = 1
  pull_policy = "if-not-present"
  [runners.custom_build_dir]
  [runners.cache]
    [runners.cache.s3]
    [runners.cache.gcs]
    [runners.cache.azure]
