{#  #}
[[runners]]
  name = "{{ hostname }}/{{ platform }}:{{ kind }}"
  url = "{{ url }}"
  token = "{{ token }}"
  executor = "docker"
  limit = 1
  environment = ["LC_ALL=en_US.UTF-8"]
  [runners.docker]
    image = "edgetoolkit/gcc5"
    tls_verify = false
    privileged = true
    disable_cache = false
    volumes = ["/certs/client", "/cache"]
    pull_policy = "if-not-present"
  [runners.cache]
    [runners.cache.s3]
    [runners.cache.gcs]
{#  #}