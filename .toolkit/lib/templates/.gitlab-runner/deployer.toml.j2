  name = "{{ hostname }}/{{ platform }}:{{ kind }}"
  url = "{{ url }}"
  token = "{{ token }}"
  executor = "docker"
  limit = {{ limit | default(1) }}
  image = "{{ docker_repository_url  }}edgetoolkit/gcc5"
{#
  pre_build_script = """set -e
  WD=$HOME/.epm/.workbench/{{workbench}}/{{ kind }}
